# Expo Access Token Configuration
# Copy this file to .env.eas and add your actual tokens
# DO NOT commit .env.eas to your repository!

# Required: Your Expo Personal Access Token
# Get this from: https://expo.dev/settings/access-tokens
EXPO_TOKEN=your_expo_access_token_here

# Optional: Apple App Store Connect API Key (for iOS credential management)
# Get this from: https://appstoreconnect.apple.com/access/api
EXPO_ASC_API_KEY_PATH=/path/to/your/AuthKey_XXXXXXXXXX.p8
EXPO_ASC_KEY_ID=your_asc_key_id
EXPO_ASC_ISSUER_ID=your_asc_issuer_id
EXPO_APPLE_TEAM_ID=your_apple_team_id
EXPO_APPLE_TEAM_TYPE=COMPANY_OR_ORGANIZATION

# Optional: Google Play Service Account (for Android submission)
# Get this from: https://console.cloud.google.com/iam-admin/serviceaccounts
EXPO_ANDROID_SERVICE_ACCOUNT_KEY_PATH=/path/to/your/service-account.json

# Build Configuration
EXPO_NO_WAIT=true
EXPO_NON_INTERACTIVE=true